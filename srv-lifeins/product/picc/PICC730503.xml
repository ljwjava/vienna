		<interest>
				<var name="NJBF" param="I" formula="I lt PAY_PERIOD ? PREMIUM : 0"/><!-- 年交保费 -->
				<var name="LJBF" param="I" formula="I lt PAY_PERIOD ? PREMIUM*(I+1) : PREMIUM*PAY_PERIOD"/><!-- 累计已交保险费（主险） -->
				<var name="LJBF1" param="I" formula="IT.LJBF(I)"/><!-- 累计已交保险费 -->
				<var name="SCBXJ" param="I">
					var now = AGE+I+1;
					if (now >= 75 or I lt 4 or now == 66)
						return 0;
					if (I == 4)
						return IT.LJBF(I)*0.2;
					return PREMIUM/UNIT*DS.DATA[0,0];
				</var>
				<var name="GABXJ" param="I" formula="AGE+I+1==66 ? IT.LJBF(I)*0.2 : 0"/><!-- 关爱保险金（年末） -->
				<var name="QMBXJ" param="I" formula="AGE+I+1==75 ? IT.LJBF(I)*0.6 : 0"/><!-- 满期保险金（年末） -->
				<var name="LJLQ" param="I" formula="(I>0?IT.LJLQ(I-1):0) + IT.SCBXJ(I)+IT.GABXJ(I)+IT.QMBXJ(I)"/><!-- 累计领取 -->
				<var name="PTSGBXJ" param="I" formula="AGE+I+1>75 ? 0 : (AGE>40?1.4:1.6)*IT.LJBF(I)"/><!-- 普通身故保险金 -->
				<var name="GGJTYWBXJ" param="I" formula="AGE+I+1>75 ? 0 : IT.LJBF(I)"/><!-- 公共交通意外身故保险金 -->
				<var name="CQHLBXJ" param="I" formula="0"/><!-- 长期护理保险金 -->
				<var name="TBJ" param="I" formula="AGE+I+1>75 ? 0 : PREMIUM/UNIT*DS.RATE[I,0]"/><!-- 退保金（主附险合并现金价值）????????? -->

				<!-- 红利 -->
				<var name="ZHJZ" param="A1,A2" formula="try(round(PREMIUM/10000*(A2==0?DS.DV1[A1,0]:(A2==1?DS.DV2[A1,0]:DS.DV3[A1,0]))),0)"/><!--账号价值（高）-->
				<!-- 红利+生存金 -->
				<var name="HL" param="I,A2" formula="I>0 ? IT.SCBXJ(I-1)+IT.GABXJ(I-1)+IT.QMBXJ(I-1)+IT.ZHJZ(I-1,A2) : 0"/><!-- 红利+生存金（低档） -->
				<!-- 追加保险费 -->
				<var name="ZJBXF" param="A1" formula="0"/>
				<!-- 其他保险费 -->
				<var name="QTBXF" param="I" formula="I==0?200:0"/>

				<!-- 账户价值低    -->
				<var name="QCZHJZ" param="I,A2" formula="IT.HL(I,A2)+IT.ZJBXF(I)+IT.QTBXF(I)-IT.CSFY(I,A2)+IT.CXJJ(I,A2)+(I>0?IT.QMZHJZ(I-1,A2):0)"/>
				<var name="QMZHJZ" param="I,A2" formula="IT.QCZHJZ(I,A2)*RATE[A2]-24-IT.FXBF(I,A2)"/><!-- 期初账户价值（低） -->
				<!-- 风险保额 -->
				<var name="FXBE" param="I,A2">I+AGE+1 le 61 ? max(IT.LJYJ(I,A2)*1.6-IT.QCZHJZ(I,A2),0) : max(IT.LJYJ(I,A2)*1.2-IT.QCZHJZ(I,A2),0)</var>
				<!-- 风险保费  IT.FXBED(I)-->
				<var name="FXBF" param="I,A2" formula="IT.FXBE(I,A2)*V4[I+AGE][GENDER-1]/1000000000000000000.0"/><!-- 风险保费（低） -->

				<!-- 持续奖金 -->
				<var name="CXJJ" param="I,A2" formula="I le 4 ? 0 : 0.005 * (I == 5 ? IT.LJYJ(I-1,A2) : IT.LJYJ(I-1,A2)-IT.LJYJ(I-2,A2))"/><!-- 持续奖金（低） -->
				<!-- 累计已交保险费-->
				<var name="LJYJ" param="I,A2" formula="IT.HL(I,A2)+IT.ZJBXF(I)+IT.QTBXF(I)+(I>0?IT.LJYJ(I-1,A2):0)"/><!-- 累计已交保险费（低） -->
				<!-- 初始费用 -->
				<var name="CSFY" param="I,A2" formula="(IT.ZJBXF(I)+IT.QTBXF(I))*0.03+IT.HL(I,A2)*0.005"/><!-- 初始费用（低） -->
		</interest>

