			<item code="input" value="THIS.YEAR" desc="'交'+THIS.YEAR+'年'" show="自定义" />
			<item code="exempt" value="PARENT.PAY_PERIOD-1" desc="'交'+round(PARENT.PAY_PERIOD-1)+'年'" show="豁免期间" />
