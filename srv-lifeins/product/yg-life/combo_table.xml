				<item code="SGZLY_BHHL" mode="add" name="不含红利" style="###############0"/>
				<item code="SGZLY_HZDLJHL" mode="add" name="含中档累积红利" style="###############0"/>
				<item code="SGZLY_HZDLJJQZE" mode="add" name="含中档累积交清增额" style="###############0"/>
			</items>
			
			<items name="意外身故总利益">
				<item code="YWSGZLY_BHHL" mode="add" name="不含红利" style="###############0"/>
				<item code="YESGZLY_HZDLJHL" mode="add" name="含中档累积红利" style="###############0"/>
				<item code="YESGZLY_HZDLJJQZE" mode="add" name="含中档累积交清增额" style="###############0"/>
			</items>
			
			<items name="保险费">
				<item code="premium" mode="add" name="期交" style="###############0"/>
				<item code="premium_acc" mode="acc" name="累计" style="###############0"/>
			</items>
		</benefit_combo>
	</attachment>
</declare>

