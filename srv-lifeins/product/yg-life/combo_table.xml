<?xml version="1.0" encoding="UTF-8"?>
<declare>
	<attachment>
		<!--共用 头 -->
		<benefit_combo filter="combo" parser="combo_def">
			<item code="year" mode="cover" name="保单年度" style="##0"/>
			<item code="age"  mode="cover" name="年末已达年龄" style="##0"/>
			
			<item code="SGBXJ" mode="add" name="身故保险金" style="###########0"/>
			<item code="BDNDCBXJ" mode="add" name="生存保险金" style="###########0"/>
			<item code="YWSGBXJ" mode="add" name="意外身故\n保险金" style="###########0"/>
			<item code="ZJBXJ" mode="add" name="重大疾病保险金" style="###########0"/>
			<item code="QZZJBXJ" mode="add" name="轻症重疾保险金" style="###########0"/>
			
			<item code="SCZLY" mode="add" name="*生存总利益" style="###########0"/>
			
			<item code="SGZLY" mode="add" name="*身故总利益" style="###########0"/>
