<?xml version="1.0" encoding="UTF-8" ?>
<declare>
	<product
		id="FSH00001"
		corporation_id="fosun"
		code="110023"
		name="康乐一生重大疾病保险（C款）"
		name_abbr="康乐一生重疾"
		unit="1000"
		type_code="thunder"
		sequence="1000"
		sale_begin_date="2017-01-01"
		sale_end_date=""
		input="amount"
		last_modify_date="2017-01-01">

		<data>
			<item parser="dds" value="FSH00001" />
		</data>

		<param>
			<pay>
				<item code="term_5" />
				<item code="term_10" />
				<item code="term_15" />
				<item code="term_20" />
				<item code="term_30" />
			</pay>
			<insure>
				<item code="to_70" />
				<item code="to_80" />
				<item code="full_105" />
			</insure>
		</param>

		<input>
			<item name="AMOUNT" label="保额" widget="select" type="integer">[[100000,'10万'],[150000,'15万'],[200000,'20万']]</item>
			<item name="PAY" label="缴费年期" widget="select">[['term_5','5年交'],['term_10','10年交'],['term_15','15年交'],['term_20','20年交'],['term_20','30年交']]</item>
			<item name="INSURE" label="保障期间" widget="select">[['to_70','保至70岁'],['to_80','保至80岁'],['full_105','保终身']]</item>
		</input>

		<attachment>
			<fgraph filter="fgraph" parser="fgraph">
				<item name="THUNDER">AMOUNT</item>
				<item name="MILD">AMOUNT</item>
			</fgraph>
		</attachment>

		<rule>
		</rule>
	</product>
</declare>
